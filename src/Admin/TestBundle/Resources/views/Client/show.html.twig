{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Client</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Pfpharlib</th>
                <td>{{ entity.pFPharLib }}</td>
            </tr>
            <tr>
                <th>Pfpharadresse</th>
                <td>{{ entity.pFPharAdresse }}</td>
            </tr>
            <tr>
                <th>Pfpharnom</th>
                <td>{{ entity.pFPharNom }}</td>
            </tr>
            <tr>
                <th>Pfpharprenom</th>
                <td>{{ entity.pFPharPrenom }}</td>
            </tr>
            <tr>
                <th>Pfphardatemaj</th>
                <td>{{ entity.pFPharDateMaj|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('client') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('client_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
