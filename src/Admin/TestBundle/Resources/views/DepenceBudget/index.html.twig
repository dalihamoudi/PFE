{% extends 'base.html.twig' %}


{% block body %}
    <h1>Depence budgets list</h1>
    {{ render(controller('AdminTestBundle:DepenceBudget:new')) }}



    <!-- HTML DATATABLE -->
    <table class="table table-striped table-bordered table-hover" id="sample_3">
        <thead>
        <tr>
            <th>
                Action
            </th>
            <th>
                Groupe de produit
            </th>
            <th>
                Date
            </th>
            <th class="hidden-xs">
                Number
            </th>
            <th class="hidden-xs">
                Prix Ht        </th>
            <th class="hidden-xs">
                Quantit√©
            </th>
            <th class="hidden-xs">
                Prix Total
            </th>


        </tr>
        </thead>
        <tbody>

        {% for entity in entities %}
            <tr>
                <td>

                    {{ render(controller('AdminTestBundle:DepenceBudget:show',{'id':entity.id})) }}
                </td>
                <td>{{ entity.GroupeProduits}}</td>
                <td>{{ entity.pFDEPENCEBUDGETDate|date('Y-m-d H:i:s')}}</td>
                <td>{{ entity.pFDEPENCEBUDGETNumber}}</td>
                <td>{{ entity.pFDEPENCEBUDGETPRIXHT}}</td>
                <td>{{ entity.pFDEPENCEBUDGETQTE}}</td>
                <td>{{ entity.pFDEPENCEBUDGETPRIXHT*entity.pFDEPENCEBUDGETQTE }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}